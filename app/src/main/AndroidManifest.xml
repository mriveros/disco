<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mriveros.disco"
    android:versionCode="2"
    android:versionName="1.1" >
    <uses-permission  android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/noctua_ico_png"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.mriveros.disco.disco.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.mriveros.disco.disco.GaleriaActivity"
            android:label="@string/app_name"
            android:parentActivityName="com.mriveros.disco.disco.MainActivity" >
        </activity>
        <activity
            android:name="com.mriveros.disco.disco.EventosActivity"
            android:label="@string/app_name"
            android:parentActivityName="com.mriveros.disco.disco.MainActivity" >
        </activity>
        <activity
            android:name="com.mriveros.disco.disco.ReservasActivity"
            android:label="@string/app_name"
            android:parentActivityName="com.mriveros.disco.disco.MainActivity" >
        </activity>
        <activity
            android:name="com.mriveros.disco.disco.EnvioReservasActivity"
            android:label="@string/app_name"
            android:parentActivityName="com.mriveros.disco.disco.ReservasActivity" >
        </activity>
        <activity
        android:name="com.mriveros.disco.disco.ProductosActivity"
        android:label="@string/app_name"
        android:parentActivityName="com.mriveros.disco.disco.MainActivity" >
    </activity>
        <activity
            android:name="com.mriveros.disco.disco.StaffActivity"
            android:label="@string/app_name"
            android:parentActivityName="com.mriveros.disco.disco.MainActivity" >
        </activity>

    </application>

</manifest>
